{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/mediahub-public/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/mediahub-public/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/mediahub-public",
  "projects": {
    "mediahub-public": {
      "type": "application",
      "root": "apps/mediahub-public",
      "entryFile": "main",
      "sourceRoot": "apps/mediahub-public/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mediahub-public/tsconfig.app.json"
      }
    },
    "user-service": {
      "type": "application",
      "root": "apps/user-service",
      "entryFile": "main",
      "sourceRoot": "apps/user-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-service/tsconfig.app.json"
      }
    }
  }
}